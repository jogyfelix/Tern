{
  "expo": {
    "name": "Tern",
    "slug": "Tern",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./app/assets/splash-screen-image.png",
      "resizeMode": "cover",
      "backgroundColor": "#00000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon .png",
        "backgroundColor": "#00000"
      }
    },
    "plugins": ["expo-router"],
    "scheme": "tern"
  }
}
